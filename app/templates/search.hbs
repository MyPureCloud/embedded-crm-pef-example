<div id="myBody" style="padding: 50px;">
    <div class="w3-twothird w3-container">
        <h1 class="w3-text-deep-orange">Search Results</h1>
        <p>{{searchResults.length}} result(s) for "{{query}}"</p>
        {{#each searchResults as |result|}}
            <li>
                {{#link-to 'contacts.contact' result.id}}
                    {{result.FirstName}} {{result.LastName}}
                {{/link-to}}
            </li>
        {{/each}}
    </div>
</div>
