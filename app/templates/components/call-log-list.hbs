<div id="myBody" style="padding-left: 35px">
  <div class="w3-twothird w3-container">
    <h1 class="w3-text-deep-orange">Call Logs</h1>
    <table>
        <thead>
            <th>Interaction ID</th>
            <th>ANI</th>
            <th>Notes</th>
            <th>Attributes</th>
            <th>Association</th>
        </thead>
        <tbody>
            {{#each callLogs as |log|}}
                <tr>
                    <td>{{log.interactionId}}</td>
                    <td>{{log.ani}}</td>
                    <td>{{log.notes}}</td>
                    <td>{{log.attr}}</td>
                    <td>{{log.assoc}}</td>
                </tr>                        
            {{/each}}
        </tbody>
    </table>
  </div>
</div>

{{yield}}